class Solution:
    def numIdenticalPairs(self, nums: List[int]) -> int:
        count=0
        freq={}
        for num in nums:
            if num in freq:
                count+=freq[num]
                freq[num]+=1
            else:
                freq[num]=1
        return count




        count=[(i,j) for i in range(len(nums)) for j in range(1,len(nums)) if nums[i]==nums[j] and i<j]
        return len(count) 




        for i in range(len(nums)):
            for j in range(1,len(nums)):
                if nums[i]==nums[j] and i<j:
                    count+=1
        return count
