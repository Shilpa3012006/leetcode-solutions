class Solution:
    def longestCommonPrefix(self, strs: List[str]) -> str:
        strs=sorted(strs)
        first=strs[0]
        last=strs[-1]
        prefix=[]
        for i in range(min(len(first),len(last))):
            if first[i]==last[i]:
                prefix.append(first[i])
            else:
                break
        return "".join(prefix)

