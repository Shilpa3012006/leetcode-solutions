class Solution:
    def runningSum(self, nums: List[int]) -> List[int]:
        for i in range(1, len(nums)):
            nums[i] += nums[i - 1]
        return nums



        running_sum=0
        res=[]
        for i in range(len(nums)):
            running_sum+=nums[i]
            res.append(running_sum)
        return res
