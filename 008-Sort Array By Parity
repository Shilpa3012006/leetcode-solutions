
class Solution:
    def sortArrayByParity(self, nums: List[int]) -> List[int]:
        e=[x for x in nums if x%2==0]
        f=[x for x in nums if x%2!=0]
        return e+f
        
'''
        e=[]
        f=[]
        for i in range(len(nums)):
            if nums[i]%2==0:
                e.append(nums[i])
            else:
                f.append(nums[i])
        return e+f
        '''
