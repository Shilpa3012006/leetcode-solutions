import bisect
class Solution:
    def searchInsert(self, nums: List[int], target: int) -> int:
        return bisect.bisect_left(nums,target)


'''
    for i in range(len(nums)):
            if nums[i]==target:
                return i
        if target<nums[0]:
            return 0
        if target>nums[len(nums)-1]:
            return len(nums)

